{
	"name": "satellite-collision-tracker-web",
	"version": "1.0.0",
	"description": "Satellite Collision Tracker Web Application",
	"main": "index.js",
	"scripts": {
		"start": "concurrently \"npm run backend\" \"npm run frontend\"",
		"backend": "node ./backend/src/js/index.js",
		"frontend": "cd frontend && npm run dev",
		"install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
		"build": "cd frontend && npm run build",
		"test": "cd frontend && npm test"
	},
	"keywords": [
		"satellite",
		"collision",
		"tracker",
		"mapbox",
		"react"
	],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"concurrently": "^8.2.0"
	}
}
